    "ts-loader": "~6.2.2",
    "vue-devtools": "5.1.3",
    "vue-eslint-parser": "~7.0.0",
    "vue-jest": "~3.0.5",
    "wait-for-expect": "^3.0.2",
    "watchpack": "^1.7.2",
    "webdriverio": "5.22.4",
    "webpack": "~4.43.0",
    "webpack-node-externals": "1.7.2"
  },
