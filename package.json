{
  "name": "MantarrayController",
  "version": "0.4.1",
  "description": "Desktop application for controlling the Mantarray instrument.",
  "author": {
    "name": "Curi Bio",
    "email": "contact@curibio.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/CuriBio/mantarray-desktop-app.git"
  },
  "homepage": "http://www.curiobio.com",
  "private": false,
  "license": "MIT",
  "main": "./dist/main/index.js",
  "scripts": {
    "prepare": "pre-commit install && npx npm-merge-driver install",
    "dev": "node ./node_modules/@curi-bio/frontend-test-utils/src/check_engines.js && cross-env-shell NODE_ENV=development node .electron-nuxt/index.js",
    "test:unit": "node ./node_modules/@curi-bio/frontend-test-utils/src/check_engines.js && cross-env-shell NODE_ENV=test jest -c jest.config.js",
    "test:e2e": "node ./node_modules/@curi-bio/frontend-test-utils/src/check_engines.js && cross-env-shell NODE_ENV=production node .electron-nuxt/index.js && cross-env-shell NODE_ENV=test jest -c jest-spectron.config.js",
    "test:e2e-no-prebuild": "node ./node_modules/@curi-bio/frontend-test-utils/src/check_engines.js && cross-env-shell NODE_ENV=test jest -c jest-spectron.config.js",
    "build": "node ./node_modules/@curi-bio/frontend-test-utils/src/check_engines.js && cross-env-shell NODE_ENV=production node .electron-nuxt/index.js",
    "build-nuxt": "node ./node_modules/@curi-bio/frontend-test-utils/src/check_engines.js && cross-env-shell NODE_ENV=production SUPPRESS_ELECTRON_BUILD=true node .electron-nuxt/index.js",
    "electron-builder-help": "electron-builder --help",
    "postinstall": "electron-builder install-app-deps",
    "org-imports": "organize-imports-cli src/main/index.dev.js"
  },
  "engines": {
    "node": ">=14.15.1"
  },
  "dependencies": {
    "@curi-bio/mantarray-frontend-components": "0.1.11",
    "@electron/remote": "1.0.2",
    "axios": "0.21.1",
    "current-script-polyfill": "1.0.0",
    "custom-electron-titlebar": "3.2.5",
    "detect-port": "1.3.0",
    "electron-store": "5.2.0",
    "electron-util": "0.14.2",
    "js-yaml": "3.14.0",
    "mkdirp": "1.0.4",
    "typeface-anonymous-pro": "1.1.13",
    "typeface-muli": "1.1.3",
    "urlsafe-base64": "1.0.0",
    "vue-axios": "2.1.5",
    "yargs": "16.2.0"
  },
  "devDependencies": {
    "@curi-bio/frontend-test-utils": "~0.1.2",
    "@fortawesome/fontawesome-free": "5.12.1",
    "@nuxtjs/eslint-config": "1.1.2",
    "@nuxtjs/vuetify": "1.11.0",
    "@types/webdriverio": "4.13.3",
    "@vue/test-utils": "1.0.3",
    "@xpda-dev/core": "0.1.5",
    "@xpda-dev/electron-builder": "0.1.1",
    "@xpda-dev/electron-launcher": "0.1.1",
    "@xpda-dev/webpack-step": "0.1.2",
    "axios-mock-adapter": "1.18.1",
    "babel-core": "7.0.0-bridge.0",
    "babel-plugin-syntax-jsx": "6.18.0",
    "babel-preset-env": "1.7.0",
    "browser-env": "3.3.0",
    "deepmerge": "4.2.2",
    "electron": "9.3.1",
    "electron-builder": "22.7.0",
    "electron-debug": "3.0.1",
    "electron-devtools-installer": "3.0.0",
    "electron-updater": "4.3.1",
    "eslint": "~7.18.0",
    "eslint-config-google": "~0.14.0",
    "eslint-config-prettier": "~7.0.0",
    "eslint-config-standard": "~16.0.2",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-plugin-import": "~2.22.1",
    "eslint-plugin-jest": "~24.1.3",
    "eslint-plugin-node": "~11.1.0",
    "eslint-plugin-promise": "~4.2.1",
    "eslint-plugin-snakecasejs": "~2.2.0",
    "eslint-plugin-vue": "~7.5.0",
    "jest": "~26.6.3",
    "jest-serializer-vue": "~2.0.2",
    "jest-sinon": "1.0.3",
    "jest-transform-stub": "~2.0.0",
    "npm-merge-driver": "2.3.6",
    "nuxt": "~2.14.12",
    "organize-imports-cli": "0.8.0",
    "prettier-plugin-organize-imports": "1.0.3",
    "proxyquire": "2.1.3",
    "python-shell": "2.0.0",
    "sinon": "~9.2.4",
    "sinon-helpers": "~2.0.4",
    "spectron": "~11.1.0",
    "style-loader": "~1.2.1",
    "tmp": "~0.2.1",
    "ts-loader": "~6.2.2",
    "vue-devtools": "~5.1.3",
    "vue-eslint-parser": "~7.4.1",
    "vue-jest": "~3.0.7",
    "wait-for-expect": "^3.0.2"
  }
}
