{
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": {
          "node": "current"
        }
      }
    ]
  ],
  "ignore": ["/node_modules/(?!colorjs.io)/.*"],
  "plugins": ["@babel/plugin-syntax-dynamic-import"],
  "env": {
    "test": {
      "presets": [
        [
          "env",
          {
            "targets": {
              "node": "current"
            }
          }
        ]
      ],
      "ignore": ["/node_modules/(?!colorjs.io)/.*"],
      "plugins": ["@babel/plugin-transform-runtime"]
    }
  }
}
